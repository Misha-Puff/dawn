      "type": "multicolumn",
      "blocks": {
        "column_bNVh4e": {
          "type": "column",
          "settings": {
            "title": "",
            "text": "<p><strong>OPEN\/CLOSED<\/strong><\/p><p>Ordering is open when our knitting partners have extra capacity<\/p>",
            "link_label": "",
            "link": ""
          }
        },
        "column_7GxhG4": {
          "type": "column",
          "settings": {
            "title": "",
            "text": "<p><strong>SELECT COLORS<\/strong><\/p><p>Each run offers beloved archival colors, repurposing our stock of excess or rejected yarns<\/p>",
            "link_label": "",
            "link": ""
          }
        },
        "column_hjKR7q": {
          "type": "column",
          "settings": {
            "title": "",
            "text": "<p><strong>LIMITED CAPACITY<\/strong><\/p><p>Handcrafted pieces and limited yarns mean a scarce supply and limited production capacity<\/p>",
            "link_label": "",
            "link": ""
          }
        },
        "column_rA7bPF": {
          "type": "column",
          "settings": {
            "title": "",
            "text": "<p><strong>LEAD TIMES<\/strong><\/p><p>Our repurposed yarn colors may be in stock or require custom dyeing, leading to a 3-5 month lead time<\/p>",
            "link_label": "",
            "link": ""
          }
        }
      },
