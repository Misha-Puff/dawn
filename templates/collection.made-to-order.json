    "image_with_text_YiwTPj": {
      "type": "image-with-text",
      "blocks": {
        "text_anCRBG": {
          "type": "text",
          "settings": {
            "text": "<p><strong>BUILDING BONDS THROUGH ETHICAL FIBERS<\/strong><\/p><p>Our MTO program helps support our knitting partners access to steady work and income on an ongoing basis beyond our seasonal collections. <\/p><p><a href=\"\/pages\/our-knitting-partners\" target=\"_blank\" title=\"Our Story - Our Knitting Partners\"><span style=\"text-decoration:underline\">Learn more<\/span><\/a> about how we empower women via our knitting cooperatives.<\/p>",
            "text_style": "body"
          }
        }
      },
      "block_order": [
        "text_anCRBG"
      ],
      "settings": {
        "image": "shopify:\/\/shop_images\/About_the_Knitting_Cooperatives_4_1224x_2x.progressive_6716fe07-03e3-4739-b489-af6667833a89.jpg",
